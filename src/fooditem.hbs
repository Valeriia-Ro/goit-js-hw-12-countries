<div class="cards">
  <h2 class="card-title">{{name}}</h2>
  <p class="card-text">Capital: {{capital}}</p>
  <p class="card-text">Population: {{population}}</p>

  <p class="card-text">Language: </p>
  <ul class="card-list">
  {{#each languages}}
  <li class="card-item"> {{languages.name}}</li>
  {{/each}}
  </ul>

</div>